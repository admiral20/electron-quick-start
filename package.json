{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "electron --inspect=5858 .",
    "packager": "electron-packager . ELECTRON-QUICK-START --platform=win32 --arch=x64  --overwrite --icon=assets/app-icon/win/app.ico",
    "builder": "electron-builder --win --x64 --dir=dist"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "build": {
    "productName": "electron-quick-start",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ]
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.3.0",
    "electron-builder": "^22.6.1",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "electron-shutdown-command": "0.0.8"
  }
}
